{
  "name": "blocks-examples",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "main": "index.js",
  "workspaces": [
    "examples/angular-uploader/app-example",
    "examples/composition/custom-source",
    "examples/js-uploader/minimal",
    "examples/js-uploader/regular",
    "examples/next-uploader/minimal",
    "examples/next-uploader/regular",
    "examples/react-uploader/app-example",
    "examples/react-uploader/minimal",
    "examples/react-uploader/regular",
    "examples/svelte-uploader/app-example",
    "examples/svelte-uploader/minimal",
    "examples/svelte-uploader/regular",
    "examples/vue-uploader/app-example",
    "examples/vue-uploader/minimal",
    "examples/vue-uploader/regular"
  ],
  "scripts": {
    "bump": "npm-check-updates /@uploadcare/ -u --deep",
    "build": "BASE_PATH=\"${BASE_PATH:=/}\" npm run build:examples && npm run build:pages",
    "build:examples": "npm run build --workspaces",
    "build:pages": "node pages.js",
    "serve": "npx serve ./public/"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "fast-glob": "^3.2.11",
    "npm-check-updates": "^16.10.8"
  }
}
